<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:newbie="http://schemas.android.com/apk/res/com.example.mobilesafe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        style="@style/title_style"
        android:text="设置中心" />

    <com.example.mobilesafe.difineView.SettingCustomView
        android:id="@+id/custom_view_autoupdate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        newbie:bg_custom_selector="first"
        newbie:display_text="自动更新服务" >
    </com.example.mobilesafe.difineView.SettingCustomView>

    <com.example.mobilesafe.difineView.SettingCustomView
        android:id="@+id/custom_view_blacklist_intercept"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        newbie:bg_custom_selector="last"
        newbie:display_text="黑名单拦截服务服务" >
    </com.example.mobilesafe.difineView.SettingCustomView>

    <com.example.mobilesafe.difineView.SettingCustomView
        android:id="@+id/custom_phone_number_display_location_service"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        newbie:bg_custom_selector="first"
        newbie:display_text="电话归属地显示服务" >
    </com.example.mobilesafe.difineView.SettingCustomView>
    
     <com.example.mobilesafe.difineView.SettingCustomView
        android:id="@+id/custom_phone_location_display_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        newbie:bg_custom_selector="last"
        newbie:is_disable_toggle="true"
        newbie:display_text="归属地显示样式(默认)" >
    </com.example.mobilesafe.difineView.SettingCustomView>

 </LinearLayout>
    